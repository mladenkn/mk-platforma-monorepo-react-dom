{
  "name": "mk-platforma-pnpm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm app-web build",
    "start": "pnpm app-web start",
    "app-web": "pnpm --filter @mk-platforma/app-web",
    "db": "pnpm --filter @mk-platforma/app-web db",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "wipe": "find . -type d -name node_modules | xargs rm -rf && rm -f pnpm-lock.lock && rm -rf packages/*/*/.next"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.26.3"
}