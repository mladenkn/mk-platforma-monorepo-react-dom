{
  "name": "mk-platforma-pnpm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "app": "pnpm --filter @mk-platforma/app-web",
    "app+": "pnpm --filter @mk-platforma/app-web +",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "wipe": "find . -type d -name node_modules | xargs rm -rf && rm -f pnpm-lock.lock && rm -rf packages/*/*/.next",
    "reset": "pnpm wipe && pnpm install"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.26.3"
}