{
  "name": "mk-platforma-pnpm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "admin-web": "pnpm --filter @mk-platforma/admin-web",
    "app-web": "pnpm --filter @mk-platforma/app-web",
    "app": "pnpm --filter @mk-platforma/app",
    "api": "pnpm --filter @mk-platforma/api",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev.wipe": "find . -type d -name node_modules | xargs rm -rf && rm -f pnpm-lock.lock && rm -rf packages/*/*/.next"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.26.3"
}